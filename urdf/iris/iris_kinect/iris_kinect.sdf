<sdf version='1.5'>
  <model name='iris_kinect'>

    <include>
      <uri>model://iris</uri>
    </include>

    <include>
      <uri>model://kinect</uri>
      <pose>0 0.0 0.07 0 0 0</pose>
    </include>
    <joint name="kinect_joint" type="revolute">
      <child>kinect::link</child>
      <parent>iris::base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>

  </model>
</sdf>
